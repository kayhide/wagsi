{
  "name": "wagsi",
  "displayName": "wagsi",
  "description": "",
  "version": "0.0.0",
  "publisher": "mikesol",
  "repository": {
    "url": "https://github.com/mikesol/wagsi"
  },
  "icon": "images/wagsi.png",
  "engines": {
    "vscode": "^1.59.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "wagsi.startLoop",
        "title": "Start Wagsi Compilation Loop"
      },
      {
        "command": "wagsi.stopLoop",
        "title": "Stop Wagsi Compilation Loop"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "extensionDependencies": [
    "nwolverson.ide-purescript"
  ],
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.59.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "vscode-languageclient": "^6.1.3",
    "vscode-languageserver-protocol": "^3.15.3"
  }
}